
type Task {
  id: ID!
  ownerID: ID!
  clientID: ID!
  dateAdded: String!
  dateCompleted: String
  completed: Boolean!
}

type Query {
    getTask(taskId: ID): Task!
    getAllTasks(ownerId: ID): [Task]!
}

type Mutation {
  createTask(
           ownerID: ID!,
           taskName: String!,
           clientName: String!
         ) : Task!
}